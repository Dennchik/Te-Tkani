extends ../layouts/app_layout
append link 
	link(rel="stylesheet" type="text/css" href="@@webRoot/css/index.min.css")
prepend script 
	script(defer="defer" type="text/javascript" src="@@webRoot/js/index.min.js") 
	//- script(defer="defer" type="text/javascript" src="@@webRoot/js/app/runtime.min.js")
	//- script(defer="defer" type="text/javascript" src="@@webRoot/js/app/anime-vendors.min.js")
	//- script(defer="defer" type="text/javascript" src="@@webRoot/js/app/react-vendors.min.js")
	//- script(defer="defer" type="text/javascript" src="@@webRoot/js/app/swiper-bundle.min.js") 
block title
	- var text='Те Ткани "Обнинск"' 

block page-content
	.page#page
		._container
			section._header
				include ../layouts/header
			section._textile 
					img(src="./img/main/textile.png", alt="картинка")
			section._new-products
				include ../components/new-products
			section._portiere-fabrics 
				include ../components/portiere-fabrics 
			section._furniture-fabrics
				include ../components/furniture-fabrics
			section._costume-fabrics
				include ../components/costume-fabrics
			section._sale-products
				include ../components/sale-products
			section._delivery
				include ../components/delivery
			section._about
				include ../components/about-page
			section._reviews
				include ../components/reviews 
			section._footer
				include ../layouts/footer 
			nav._buttom-menu
				include ../layouts/float-menu
			
				
